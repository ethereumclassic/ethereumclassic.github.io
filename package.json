    "@mdx-js/tag": "^0.20.3",
    "@reach/router": "^1.3.3",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^3.1.3",
    "gatsby": "^2.20.18",
    "gatsby-image": "^2.3.2",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-feed": "^2.4.1",
